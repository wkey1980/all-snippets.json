			"",
			"    // Register Theme Scripts",
			"    wp_register_script( '${2:scriptName}', ${3:textDomain}_THEME_URI . '${4:filePath}', array('${5:jquery}'), filemtime( get_template_directory() . '${4:filePath}' ), ${6:true/false}  );",
